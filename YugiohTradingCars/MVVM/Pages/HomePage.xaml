<Page x:Class="YugiohTradingCars.MVVM.Pages.HomePage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:YugiohTradingCars.MVVM.Pages"
      xmlns:viewModel="clr-namespace:YugiohTradingCars.MVVM.ViewModels"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="HomePage">

    <Page.Resources>
        <ResourceDictionary>
            <viewModel:HomeViewModel x:Key="ViewModel"/>
        </ResourceDictionary>
    </Page.Resources>

    <Page.DataContext>
        <Binding Source="{StaticResource ViewModel}" x:Name="vm"/>
    </Page.DataContext>


    <Label Content="home page"/>
</Page>
